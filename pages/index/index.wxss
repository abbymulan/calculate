.row{
  display: flex;
  flex-direction: row;
  flex: 1;
  width: 100%;
  height: 185rpx;
  background-color: #eee
}
.row view,.row navigator{
  width: 25%;
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: center;
  margin-top: 1px;
  margin-right: 1px;
}
.row view:active{
  background-color: #ff0000
}
.orange{
  background: #f77d11;
}
.blue{
  background-color: #0099cc
}
